@model List<ResultServiceDto>
@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/AdminLayout/_AdminLayout.cshtml";
}

<table class="table table-bordered">
    <tr>
        <th>#</th>
     
        <th>Hizmet İkon</th>
        <th>Hizmet Başlık</th>
        <th>Hizmet Açıklama</th>
        <th>Sil</th>
        <th>Güncelle</th>
       
    </tr>
    @foreach (var x in Model)
    {
        <tr>
            <th>@x.ServiceId</th>
           
            <td>@x.ServiceIcon</td>
            <td>@x.Title</td>
            <td>@x.Description</td>
            <td><a href="/Service/DeleteService/@x.ServiceId" class="btn mb-1 btn-rounded btn-outline-danger">Sil</a></td>
            <td><a href="/Service/UpdateService/@x.ServiceId" class="btn mb-1 btn-rounded btn-outline-success">Güncelle</a></td>
           
        </tr>
    }
</table>

<a href="/Service/AddService/" class="btn mb-1 btn-rounded btn-outline-primary">Yeni Hizmet Girişi</a>

